{
  "name": "lightrail-client",
  "version": "1.1.0",
  "description": "A Javascript and Typescript client for Lightrail",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist",
    "lint": "tslint --project tsconfig.json",
    "prepublishOnly": "npm run clean && npm run build && npm run lint && npm run test",
    "test": "mocha --timeout=5000 --recursive --throw-deprecation --require ts-node/register --require dotenv/config \"src/**/*.test.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Giftbit/lightrail-client-javascript.git"
  },
  "keywords": [
    "lightrail"
  ],
  "contributors": [
    "Jeffery Grajkowski <pushplay@gmail.com> (https://github.com/pushplay)",
    "Tana Jukes <tana.jukes@gmail.com> (https://github.com/tjukes)",
    "Tim Jordison (https://github.com/Tim-Jordison)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Giftbit/lightrail-client-javascript/issues"
  },
  "homepage": "https://github.com/Giftbit/lightrail-client-javascript#readme",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.44",
    "@types/superagent": "^3.5.6",
    "@types/node": "^8.0.28",
    "@types/chai-as-promised": "^7.1.0",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^4.0.0",
    "rimraf": "^2.6.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "uuid": "^3.1.0"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^7.2.3",
    "jsonwebtoken": "^8.1.0",
    "superagent": "^3.8.1"
  },
  "optionalDependencies": {
    "superagent-logger": "^1.1.0"
  }
}
